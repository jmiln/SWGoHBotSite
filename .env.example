# Server configuration
PORT=3300
NODE_ENV=production

# Path to bot's help.json file (REQUIRED)
BOT_DATA_PATH=/path/to/bot/data/help.json

# Discord OAuth2
DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=
DISCORD_REDIRECT_URI=http://localhost:3300/callback

# MongoDB (for session storage)
MONGODB_URI=mongodb://localhost:27017/swgohbot

# Session
SESSION_SECRET=

# Discord Bot Token (for resolving role/channel names in guild config viewer)
DISCORD_BOT_TOKEN=

# MongoDB database name for SWAPI unit data (for resolving unit names in TW list/aliases)
MONGODB_SWAPI_DB=
